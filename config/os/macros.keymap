        compatible = "zmk,behavior-macro";
        #binding-cells = <0>;
        bindings
                = <&macro_press &mo LOWER &kp RALT>
                , <&macro_pause_for_release>
                , <&macro_release &mo LOWER &kp RALT>;
};

