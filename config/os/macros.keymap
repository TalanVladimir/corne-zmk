shutdown_c: shutdown_c {
        label = "ZM_shutdown_c";
        compatible = "zmk,behavior-macro";
        #binding-cells = <0>;
        bindings
                = <&macro_tap &kp LC(LA(T))>
                , <&macro_tap &kp SPACE &kp S &kp H &kp U &kp T &kp D &kp O &kp W &kp N &kp SPACE &kp MINUS &kp C &kp AMPERSAND &kp AMPERSAND &kp SPACE &kp E &kp X &kp I &kp T>
                , <&macro_tap &kp ENTER>;
};

